{
  "name": "scully-portfolio-v2",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "dev": "ng serve",
    "build": "ng build",
    "build:ci": "rm -rf dist/ && ng build && npx scully --scan",
    "build:remove": "rm -rf dist/",
    "build:static": "yarn build:remove && ng build && npx scully",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "cypress": "npx cypress open",
    "reformat": "npx prettier --write .",
    "scully": "npx scully",
    "scully:scan": "npx scully --scan",
    "scully:serve": "npx scully serve",
    "lint": "ng lint",
    "prepare": "husky install"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.0.0",
    "@angular/common": "^15.0.0",
    "@angular/compiler": "^15.0.0",
    "@angular/core": "^15.0.0",
    "@angular/forms": "^15.0.0",
    "@angular/platform-browser": "^15.0.0",
    "@angular/platform-browser-dynamic": "^15.0.0",
    "@angular/router": "^15.0.0",
    "@scullyio/init": "2.1.36",
    "@scullyio/ng-lib": "^2.1.0",
    "@scullyio/scully": "^2.1.0",
    "@scullyio/scully-plugin-puppeteer": "^2.1.36",
    "lottie-web": "^5.9.6",
    "ng-zorro-antd": "14.0.0",
    "ngx-lottie": "^9.0.1",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.0.0",
    "@angular-eslint/builder": "15.0.0",
    "@angular-eslint/eslint-plugin": "15.0.0",
    "@angular-eslint/eslint-plugin-template": "15.0.0",
    "@angular-eslint/schematics": "15.0.0",
    "@angular-eslint/template-parser": "15.0.0",
    "@angular/cli": "~15.0.0",
    "@angular/compiler-cli": "^15.0.0",
    "@types/jasmine": "~4.0.0",
    "@typescript-eslint/eslint-plugin": "5.36.2",
    "@typescript-eslint/parser": "5.36.2",
    "cypress": "^10.9.0",
    "eslint": "^8.23.0",
    "husky": ">=6",
    "jasmine-core": "~4.2.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "lint-staged": ">=10",
    "prettier": "^2.7.1",
    "typescript": "~4.8.4"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{ts,js,scss,css,html,md}": "prettier --write"
  }
}